<h1>Leads#edit</h1>
<p>Find me in app/views/leads/edit.html.erb</p>
<%= form_for @lead, url: owner_lead_path(@owner) do |f| %>
  <%= f.label :lead_name %>:
  <%= f.text_field :lead_name %><br />

  <%= f.label :lead_number %>:
  <%= f.text_field :lead_number %><br />

  <%= f.label :lead_email %>:
  <%= f.text_field :lead_email %><br />

  <%= f.label :lead_web %>:
  <%= f.text_field :lead_web %><br />

  <%= f.label :lead_temp %>:
  <%= f.text_field :lead_temp %><br />

  <%= f.submit "Submit" %>
<% end %>